{
  "name": "copy-polymarket",
  "version": "0.0.1",
  "description": "Telegram Bot with Redis and BullMQ",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "bids": "tsx src/scripts/printBids.ts",
    "redeem-check": "tsx src/scripts/redeemCheck.ts",
    "clear-locks": "tsx src/scripts/clearTaskLocks.ts",
    "show-tasks": "tsx src/scripts/showTasksAndPositions.ts"
  },
  "keywords": [
    "telegram",
    "redis",
    "bullmq",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.23.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@polymarket/clob-client": "^5.2.1",
    "axios": "^1.13.2",
    "bullmq": "^5.66.5",
    "dotenv": "^17.2.3",
    "ethers": "^6.16.0",
    "ioredis": "^5.9.1",
    "mongoose": "^9.1.3",
    "redis": "^5.10.0",
    "telegraf": "^4.16.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.6",
    "@types/uuid": "^11.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
